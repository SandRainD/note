import java.awt.BorderLayout;
import java.awt.Color;
import java.awt.Container;
import javax.swing.*;
class Demo extends JFrame{
	public static void main(String[] args) {
		Demo d=new Demo();
		d.setVisible(true);
	}
	public Demo() {
		setTitle("操作表格");
		setBounds(100,100,500,375);
		setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
		
		String[] columnNames= {"A","B","C","D","E"};
		String[][] tableValues=new String[20][columnNames.length];
		
		for(int row=0;row&lt;tableValues.length;row++) {
			for(int column=0;column&lt;columnNames.length;column++) {
				tableValues[row][column]=columnNames[column]+row;
			}
   
   
   
   
		}
		JTable table=new JTable(tableValues,columnNames);
		Container c=getContentPane();
		JScrollPane p=new JScrollPane(table);
		c.add(BorderLayout.CENTER,p);
		
		table.setSelectionForeground(Color.red);
		table.setSelectionBackground(Color.yellow);
		table.setRowHeight(9);//设置行高
		/*
		 * 设置选择模式
		 */
		table.setSelectionMode(ListSelectionModel.MULTIPLE_INTERVAL_SELECTION);
		System.out.println(""+table.getRowCount()+table.getColumnCount());//获取行和列的数量
		System.out.println(table.getColumnName(1));//获取第二列的索引
		System.out.println(table.getValueAt(1,1));//获取第二行第二列的数值 
	}
}
