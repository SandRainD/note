    public Demo(){
        setTitle();设置窗体标题
        setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);//关闭窗体，停止程序
        setBounds(int,int,int,int);//窗体位置和大小
        Container c=getContentPane();//获取窗体的容
        
        c.setLayout(new BorderLayout());//设置为边界布局，也是默认布局
        JButton b1=new JButton("sdf");
        JButton b2=new JButton("sdf");
        JButton b3=new JButton("sdf");
        JButton b4=new JButton("sdf");
        JButton b5=new JButton("sdf");
        c.add(b1,BorderLayout.CENTER);
        c.add(b2,BorderLayout.EAST);
        
        setVisible(true);//设置窗体可见
    }
    public void static main(String[] args){
        new Demo();
    }
}
