    JFrame f=new JFrame();//主窗体
    Container c;//主容器
    
    void createFrame(){
        c=f.getContentPane();
        c.setLayout(new GridBagLayout());
        f.setSize(800,600);
        f.setLocationRelativeTo(null);//设置居中
        f.setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
    }
    void createButton(){//放满第一行第一列，防止自动的隐藏无内容元素
        for(int i=0;i&lt;9;i++){
            GridBagConstraints g1=new GridBagConstraints();
            g1.gridx=i;
            g1.gridy=0;
            c.add(new JButton("组件"),g1);
            
            GridBagConstraints g2=new GridBagConstraints();
            g2.gridx=0;
            g2.gridy=i;
            c.add(new JButton("组件"),g2);
        }
    }
    void init3(){
        GridBagConstraints g1=new GridBagConstraints();
        g1.gridx=1;g1.gridy=1;
        g1.gridwidth=2;g1.gridhieght=2;
        c.add(new JButton("none"),g1);
        
        GridBagConstraints g2=new GridBagConstraints();
        g2.gridx=3;g2.gridy=1;   //索引从0开始
        g2.gridwidth=2;g2.gridheight=2;
        g2.fill =GridBagConstraints.HORIZONTAL;//水平填充
        c.add(new JButton("HORIZONTAL"),g2);
        
        GridBagConstraints g3=new GridBagConstraints();
        g3.gridx=5;g3.gridy=1; //没有组件的部分会被隐藏
        g3.gridwidth=2;g3.height=2;
        g2.fill =GridBagConstraints.VERTICAL;//垂直填充
        c.add(new JButton("VERTICAL"),g3);
        
        GridBagConstraints g4=new GridBagConstraints();
        g3.gridx=7;g3.gridy=1; //没有组件的部分会被隐藏
        g3.gridwidth=2;g3.height=2;
        g2.fill =GridBagConstraints.BOTH;//全部填充
        c.add(new JButton("BOTH"),g3);
    }
    
    public void static main(String[] args){
        Demo d=new Demo();
        d.createFrame();
        d.createButton();
        d.init3();
        d.f.setVisible()
    }
}
